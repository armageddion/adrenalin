{
	"compilerOptions": {
		"target": "esnext",
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"lib": ["esnext", "dom"],
		"rootDir": "./",
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"strict": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules", "dist"]
}
